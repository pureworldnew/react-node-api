{
  "name": "react-node-boilerplate",
  "version": "1.0.0",
  "description": "react and node api",
  "main": "index.js",
  "scripts": {
    "server:install": "npm install",
    "client:install": "cd ui-react && npm install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "client:dev": "cd ui-react && npm start",
    "server:start": "node -r esm index.js",
    "server:dev": "nodemon -r esm index.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "dev": "concurrently \"npm run server:dev\" \"npm run client\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^6.3.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.11",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.2",
    "nodemon": "^2.0.13",
    "sequelize": "^6.7.0",
    "sequelize-auto": "^0.8.4",
    "sequelize-cli": "^6.2.0"
  }
}
